#include <iostream>
using namespace std;

int main()
{
	float a, b, sum, razn, pr, chast;
	cin >> a >> b;
	sum = abs(a)+abs(b);
	razn = abs(a)-abs(b);
	pr = abs(a)*abs(b);
	chast = abs(a) / abs(b);
	cout << "sum=" << sum << " " << "razn=" << razn << " " << "pr=" << pr << " " << "chast=" << chast;
	return 0;
}
